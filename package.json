{
  "name": "easily-expect",
  "version": "1.0.4",
  "description": "You can easily use expect",
  "main": "index.js",
  "scripts": {
    "test": "npm run standard && node ./node_modules/mocha/bin/mocha $(find ./test -name '*.js') --require mocha-clean --reporter=mocha-better-spec-reporter",
    "standard": "standard \"./*.js\" \"test/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KSH-code/easily-expect.git"
  },
  "author": "KSH-Code <tjdgnsqn133@gmail.com> (github.com/ksh-code)",
  "license": "MIT",
  "keywords": [
    "expect",
    "assert",
    "easily",
    "objects",
    "object",
    "check"
  ],
  "dependencies": {
    "chai": "^4.1.2",
    "easily-js": "^1.0.2",
    "fs": "0.0.1-security",
    "mocha-better-spec-reporter": "^3.1.0",
    "mocha-clean": "^1.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "mocha": "^4.1.0",
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "checkEmpty.js",
      "checkNotEmpty.js"
    ]
  }
}
